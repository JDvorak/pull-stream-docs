{
  "name": "pull-stream-docs",
  "version": "0.0.0",
  "description": "pull stream ecosystem docs",
  "main": "index.js",
  "scripts": {
    "test": "tape test.js",
    "start": "budo example:bundle.js -d example --live -- -d",
    "bundle": "browserify example -g envify -g uglifyify",
    "build": "npm run --silent bundle -- -o example/bundle.js",
    "disc": "npm run --silent bundle -- --full-paths | discify --open",
    "push": "gh-pages -d example",
    "deploy": "npm-run-all -s build push"
  },
  "browserify": {
    "transform": []
  },
  "remarkConfig": {
    "settings": {
      "commonmark": true,
      "bullet": "*"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/pull-stream-docs.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/pull-stream-docs/issues"
  },
  "homepage": "https://github.com/ahdinosaur/pull-stream-docs#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "budo": "^8.1.0",
    "disc": "^1.3.2",
    "ecosystem-docs": "^1.1.1",
    "envify": "^3.4.0",
    "gh-pages": "^0.11.0",
    "ghauth": "^3.2.0",
    "npm-run-all": "^1.6.0",
    "remark": "^4.2.2",
    "run-waterfall": "^1.1.3",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "inu": "^2.3.1",
    "pull-stream": "^3.4.0"
  }
}
